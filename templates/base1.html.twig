<!DOCTYPE html>
<html>
	<head>
		<meta charset="UTF-8">
		<title>CUTURL-
			{% block title %}{% endblock %}
		</title>
		<link
		rel="icon" href="/images/cat2.png" type="image/x-icon">
		<!-- Bootstrap core CSS -->
		<link rel="stylesheet" href="/bootstrap/css/bootstrap.min.css">

		{% block stylesheets %}
			{{ encore_entry_link_tags('app') }}
		{% endblock %}

		{% block javascripts %}
			{{ encore_entry_script_tags('app') }}
		{% endblock %}
		<script type="text/javascript">
			let globalData;
		</script>
		<!-- JQuery -->

		<!-- Bootstrap tooltips -->
		<!-- Bootstrap core JavaScript --><script type="text/javascript" src="/bootstrap/js/bootstrap.min.js"> </script>
		<script type="text/javascript" src="/js/jquery-3.6.0.js"></script>
		<script src="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/js/bootstrap-datepicker.min.js"></script>
		<link href="https://cdnjs.cloudflare.com/ajax/libs/bootstrap-datepicker/1.6.4/css/bootstrap-datepicker.min.css" rel="stylesheet"/>

	</head>
	<body>
		<header>
			<nav class="navbar navbar-expand-lg navbar-dark bg-dark fixed-top ">
				<div class="container-fluid">
					<a class="navbar-brand" href="{{path('app_homepage') }}"><img src="/images/logo17.png" alt="" width="80" height="30"></a>
					<button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false" aria-label="Toggle navigation">
						<span class="navbar-toggler-icon"></span>
					</button>
					<div class="collapse navbar-collapse" id="navbarSupportedContent">
						<ul class="navbar-nav me-auto mb-2 mb-lg-0">
							{% if is_granted('IS_AUTHENTICATED_FULLY') %}
								<li class="nav-item">
									<a class="nav-link " href="{{path('app_homepage') }}">Accueil</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_solutions')}}">Solutions</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_traits')}}">Les Traits</a>
								</li>
								<li class="nav-item">
									{% if app.user.urls.count > 0 %}
										<a class="nav-link" href="{{ path('url_list') }}">Mes Liens</a>
									{% endif %}
								</li>
							{% else %}
								<li class="nav-item">
									<a class="nav-link " href="{{path('app_homepage') }}">Accueil</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_tarification')}}">Tarification</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_solutions')}}">Solutions</a>
								</li>
								<li class="nav-item">
									<a class="nav-link" href="{{path('app_traits')}}">Les Traits</a>
								</li>
							{% endif %}
						</ul>
					</div>
				</div>
			</div>

			<div class="collapse navbar-collapse me-5" id="navbarNavDarkDropdown">
				<ul class="navbar-nav ">
					<li class="nav-item dropdown">
						<a class="nav-link dropdown-toggle" href="#" id="navbarDarkDropdownMenuLink" role="button" data-bs-toggle="dropdown" aria-expanded="false">
							<img src="/images/avatar1.jpg" class="rounded-circle" height="35" alt="Black and White Portrait of a Man" loading="lazy"/>
						</a>
						<ul class="dropdown-menu dropdown-menu-dark me-5 " aria-labelledby="navbarDarkDropdownMenuLink">
							{% if is_granted('IS_AUTHENTICATED_FULLY') %}
								<li>
									<a class="dropdown-item" href="{{ path('app_profil') }}">{{app.user.prenom}} {{app.user.nom}}</a>

								</li>
								<li>
									<a class="dropdown-item" href="{{ path('app_edit') }}">Paramètres</a>
								</li>
								<li>
									<a class="dropdown-item" href="{{ path('app_logout') }}">Déconnexion</a>
								</li>
							{% else %}
								<li>
									<a class="dropdown-item" href="{{ path('app_tarification') }}">Inscription</a>
									
								</li>
								<li>
									<a class="dropdown-item" href="{{ path('app_login') }}">Connexion</a>
								</li>
							{% endif %}
						</ul>
					</li>
				</ul>
			</div>
		</body>
	</body>
</html></nav>
</body></html></header>{% block body %}{% endblock %} 
